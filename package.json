{
    "name": "@eigenspace/jest-screenshot-testing",
    "version": "0.1.9",
    "private": false,
    "scripts": {
        "build": "webpack --config ./webpack.config.package.js",
        "lint": "tslint --project tsconfig.json && yarn lint:markdown",
        "lint:fix": "tslint --project tsconfig.json --fix",
        "lint:markdown": "node ./node_modules/@eigenspace/codestyle/scripts/markdown-lint.js",
        "test": "jest",
        "build:publish": "yarn lint && yarn test && yarn build && npm publish ./dist --access public",
        "hooks:pre-push": "yarn lint && yarn test",
        "script:update-deps-remote": "npx -p @eigenspace/helper-scripts update-deps"
    },
    "husky": {
        "hooks": {
            "pre-push": "yarn hooks:pre-push",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "yarn lint:fix",
            "git add"
        ]
    },
    "dependencies": {
        "enzyme": "3.8.0",
        "enzyme-to-json": "3.3.5",
        "jest": "24.7.1",
        "jest-image-snapshot": "2.8.1",
        "react": "16.8.1",
        "react-dom": "16.8.1",
        "react-test-renderer": "16.8.1"
    },
    "devDependencies": {
        "@eigenspace/codestyle": "latest",
        "@types/enzyme": "3.1.17",
        "@types/jest": "22.2.2",
        "@types/jest-image-snapshot": "2.8.0",
        "@types/react": "16.8.2",
        "@types/react-dom": "16.8.0",
        "@types/react-test-renderer": "16.8.0",
        "@types/styled-components": "4.1.8",
        "clean-webpack-plugin": "1.0.1",
        "copy-webpack-plugin": "4.6.0",
        "husky": "1.3.1",
        "ts-jest": "24.0.2",
        "ts-loader": "5.3.3",
        "tslint": "5.12.1",
        "lint-staged": "8.1.5",
        "typescript": "3.3.3",
        "webpack": "4.29.3",
        "webpack-cli": "3.2.3",
        "whatwg-fetch": "3.0.0"
    },
    "main": "index.js",
    "types": "index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/eigen-space/jest-screenshot-testing.git"
    },
    "license": "MIT"
}
