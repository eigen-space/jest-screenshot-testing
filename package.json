{
    "name": "@eigenspace/jest-screenshot-testing",
    "version": "0.1.15",
    "private": false,
    "scripts": {
        "build": "yarn compile && yarn copy:assets && yarn script:remove-garbage-declaration && yarn bundle-dts",
        "copy:assets": "node ./dev/scripts/copy-assets.js",
        "compile": "tsc",
        "bundle-dts": "node node_modules/@eigenspace/helper-scripts/scripts/project/bundle-dts.js",
        "lint:code": "eslint --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "yarn lint:code --fix",
        "lint:markdown": "node ./node_modules/@eigenspace/codestyle/scripts/markdown-lint.js",
        "lint": "yarn lint:code . && yarn lint:markdown",
        "test": "jest",
        "script:remove-garbage-declaration": "node node_modules/@eigenspace/helper-scripts/scripts/project/remove-files-by-pattern.js --searchDir ./dist --pattern \"(stub|spec).d.ts\"$",
        "deploy:publish": "node node_modules/@eigenspace/helper-scripts/scripts/ci/publish.js",
        "hooks:pre-push": "yarn lint && yarn test",
        "script:update-deps-remote": "npx -p @eigenspace/helper-scripts update-deps"
    },
    "husky": {
        "hooks": {
            "pre-push": "yarn hooks:pre-push",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "yarn lint:fix",
            "git add"
        ]
    },
    "dependencies": {
        "@eigenspace/jest-testing-utils": "0.0.1",
        "enzyme": "3.8.0",
        "enzyme-to-json": "3.3.5",
        "jest-image-snapshot": "2.8.1",
        "react-test-renderer": "16.8.1"
    },
    "devDependencies": {
        "@eigenspace/codestyle": "latest",
        "@eigenspace/common-types": "0.0.5",
        "@eigenspace/eslint-config-codestyle": "latest",
        "@eigenspace/helper-scripts": "1.0.77",
        "@types/enzyme": "3.1.17",
        "@types/jest": "22.2.2",
        "@types/jest-image-snapshot": "2.8.0",
        "@types/react": "16.8.3",
        "@types/react-dom": "16.8.3",
        "@types/react-test-renderer": "16.8.1",
        "@types/styled-components": "4.1.8",
        "eslint": "5.16.0",
        "husky": "1.3.1",
        "jest": "24.7.1",
        "jest-styled-components": "6.3.3",
        "lint-staged": "8.1.5",
        "react": "16.8.1",
        "react-dom": "16.8.1",
        "styled-components": "4.1.3",
        "ts-jest": "24.0.2",
        "typescript": "3.3.3",
        "whatwg-fetch": "3.0.0"
    },
    "peerDependencies": {
        "jest": "24.x",
        "jest-styled-components": ">= 6.3.3",
        "react": "16.x",
        "react-dom": "16.x",
        "styled-components": "4.x"
    },
    "main": "index.js",
    "types": "index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/eigen-space/jest-screenshot-testing.git"
    },
    "license": "MIT"
}